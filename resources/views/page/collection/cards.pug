link(rel="stylesheet", href="css/page/collection/cards.css")
script(src="js/page/collection/cards.js")
section(class="collection_cards", data-title="Supreme")
  each collection in collections
    div(class="card")
      ul(class="images hidden")
        each image in collection.images
          li
            = image
      ul(class="properties hidden")
        each prop in collection.props
          li
            = prop
      div(class="text hidden")
        = collection.text
      span(class="title")
        = collection.title
      a(href=`${collection.link}`, class="button circuit btn-link")
        span(class="span-md")
          | В карточку товара
        span(class="span-sm")
          | В карточку
        include ../../partials/svg/arrow-bottom
      button(type="button", class="button circuit btn-popup")
        span
          | Быстрый просмотр
        include ../../partials/svg/modal
      img(src=`${collection.images[0]}`, alt=`${collection.title}`, class="img")